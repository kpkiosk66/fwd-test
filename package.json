{
  "name": "fwd-react-test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:frontend": "vite",
    "dev:backend": "nodemon server/index.ts",
    "dev": "concurrently 'npm:dev:frontend' 'npm:dev:backend'",
    "test:frontend": "jest --verbose --rootDir=src",
    "test:backend": "jest --verbose --rootDir=server --config=jest.node.config.js --runInBand --force-exit",
    "test": "jest --verbose --rootDir=src && jest --verbose --rootDir=server --config=jest.node.config.js --runInBand --force-exit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/supertest": "^6.0.2",
    "axios": "^1.6.7",
    "babel-jest": "^29.7.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "flowbite": "^2.3.0",
    "flowbite-react": "^0.7.3",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "mongoose": "^8.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "redux-mock-store": "^1.5.4",
    "supertest": "^6.3.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/redux-mock-store": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "enzyme": "^3.11.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "postcss": "^8.4.37",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
